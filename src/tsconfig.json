{
	"compilerOptions": {
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": false,
		"removeComments": false,
		"preserveConstEnums": true,
		"target": "es5",
		"strictNullChecks": false,
		"noUnusedParameters": false,
		"noUnusedLocals": true
	},
	"exclude": [
		"node_modules",
		".vscode-test"
	]
}
